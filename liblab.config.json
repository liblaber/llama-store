{
  "sdkName": "llama-store",
  "specFilePath": "spec.json",
  "languages": [
    "typescript",
    "python",
    "java"
  ],
  "auth": [
    "bearer"
  ],
  "createDocs": false,
  "customizations": {
    "devContainer": true,
    "license": {
      "type": "MIT"
    }
  },
  "languageOptions": {
    "typescript": {
      "bundle": true,
      "exportClassDefault": true,
      "githubRepoName": "llama-store-sdk-typescript",
      "sdkVersion": "0.0.1"
    },
    "python": {
      "pypiPackageName": "LlamaStore",
      "githubRepoName": "llama-store-sdk-python",
      "sdkVersion": "0.0.1"
    },
    "java": {
      "groupId": "com.liblab",
      "githubRepoName": "llama-store-sdk-java",
      "sdkVersion": "0.0.1"
    }
  },
  "publishing": {
    "githubOrg": "liblaber"
  }
}